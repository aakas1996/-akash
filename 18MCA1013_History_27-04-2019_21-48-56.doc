<script language='javascript'>var arr=[];</script>
<script>
var start=0;
var table = document.getElementById("hist");
for(i=0;i<arr.length;i++)
{
	for (j=start;j<table.rows.length;j++)
	{
		if (table.rows[j].cells[1].innerHTML==arr[i][0])
		{
			if (table.rows[j].cells[5].innerHTML==arr[i][1] && table.rows[j].cells[5].innerHTML=='N')
			{
				table.rows[j].cells[5].style.backgroundColor='Yellow';
				start=j+1;
				break;
			}
			else if (table.rows[j].cells[5].innerHTML==arr[i][1] && table.rows[j].cells[5].innerHTML=='F')
			{
				table.rows[j].cells[5].style.backgroundColor='Cyan';
				start=j+1;
				break;
			}
			
		}
	}
}		
</script>
